# Exporting everything.
--svn-sync-process-export all

# This directory.
/undefined_*:                       --treat-as hashtext --external-copyright --del-action force-native-eol --add-action force-lf
/undefined_*:                       --no-strip-trailing-blanks # bugs in content!

# vboxvideo
/vboxvideo/*.c|/vboxvideo/*.h: --license-mit
/vboxvideo/vboxvideo*.c|/vboxvideo/vboxvideo*.h: --license-based-on-mit --no-convert-tabs --dont-set-svn-keywords
/vboxvideo/edid.c: --license-based-on-mit
/vboxvideo/setmode.c: --license-based-on-mit

/VBoxClient/display-svga-xf86cvt.cpp: --license-based-on-mit

# Installer
/Installer/*.conf:                  --treat-as hashtext  --del-action force-native-eol --add-action force-lf --external-copyright
/Installer/*.desktop:               --treat-as plaintext --del-action force-native-eol --add-action force-lf
/Installer/98vboxadd-xclient:       --treat-as .sh
/Installer/vboxvideo.ids:           --treat-as plaintext --del-action force-native-eol --add-action force-lf

# wlInclude
/wlInclude/*:                       --3rd-party

# x11include
/x11include/*:                      --3rd-party
/x11include/*.h.in:                 --treat-as .h
/x11include/XFree86-4.3/*:          --skip-unicode-checks

