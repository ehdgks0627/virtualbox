# Some external stuff.
/main.c:                --external-copyright
/svnmirror.sh:          --external-copyright
/svn_private_config.h:  --external-copyright --no-fix-header-guards

# Shell scripts are always fun.
/init-vbox-ose*:        --svn-sync-process-export none --treat-as shell
/pre-revprop-change:    --svn-sync-process-export none --treat-as shell
/sync-vbox-ose:         --svn-sync-process-export none --treat-as shell
/update-virtualbox.org: --svn-sync-process-export none --treat-as shell
/*.cfg:                 --svn-sync-process-export none --treat-as hashtext --external-copyright
/*.sh:                  --svn-sync-process-export none
